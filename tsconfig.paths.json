{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/pages/*": [
        "pages/*"
      ],
      "@/components": [
        "src/view/components"
      ],
      "@/view/*": [
        "src/view/*"
      ],
      "@/config": [
        "src/config"
      ],
      "@/constants/*": [
        "src/constants/*"
      ],
      "@/types": [
        "src/types"
      ],
      "@/themes": [
        "src/themes"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/context": [
        "src/context"
      ],
      "@/hooks": [
        "src/hooks"
      ],
      "@/data": [
        "src/data"
      ],
      "@/tests/*": [
        "tests/*"
      ]
    }
  }
}